<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quotes</title>
    <link rel="stylesheet" href="output.css">
    <style>
        .quote-card {
            background: rgba(255, 255, 255, 0.8);
            backdrop-filter: blur(10px);
        }
    </style>
</head>
<body class="bg-gray-100">
    <div class="container mx-auto mt-10">
        <div class="bg-white p-8 rounded-lg shadow-lg relative">
            <div class="absolute top-4 right-4">
                <form action="/logout" method="post">
                    <button type="submit" class="bg-blue-700 text-white px-4 py-2 rounded hover:bg-blue-500">Logout</button>
                </form>
            </div>
            <h1 class="text-2xl font-bold">Welcome, <%= locals.name %></h1>
            <h2 class="text-xl">Your email: <%= locals.email %></h2>
        </div>

        <div id="quote-container" class="mt-10 grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
            <!-- Quotes will be appended here -->
        </div>
    </div>

    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const quoteContainer = document.getElementById('quote-container');

            const staticQuotes = [
                {
                    content: "The greatest glory in living lies not in never falling, but in rising every time we fall.",
                    author: "Nelson Mandela",
                    image: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRuj_I8gHLrw2JAm-EwkVHQB-ZD7hVMaTX68w&s"
                },
                {
                    content: "The way to get started is to quit talking and begin doing.",
                    author: "Walt Disney",
                    image: "data:image/webp;base64,"
                },
                {
                    content: "Your time is limited, so don't waste it living someone else's life.",
                    author: "Steve Jobs",
                    image: "data:image/webp;base64,"
                },
                {
                    content: "If life were predictable it would cease to be life, and be without flavor.",
                    author: "Eleanor Roosevelt",
                    image: "data:image/jpeg;base64,"
                },
                {
                    content: "If you look at what you have in life, you'll always have more.",
                    author: "Oprah Winfrey",
                    image: "https://encrypted-tbn2.gstatic.com/images?q=tbn:ANd9GcS3icMEAfQn6dVuQHKTIllNgkvGXh1Rt2o0kc5m_wbcK7Fzk8-r"
                },
                {
                    content: "If you set your goals ridiculously high and it's a failure, you will fail above everyone else's success.",
                    author: "James Cameron",
                    image: "data:image/jpeg;base64,"
                }
            ];

            function displayQuotes(quotes) {
                for (const quote of quotes) {
                    const quoteCard = document.createElement('div');
                    quoteCard.classList.add('quote-card', 'p-6', 'rounded-lg', 'shadow-lg', 'bg-white', 'relative');

                    quoteCard.innerHTML = `
                        <img src="${quote.image}" alt="Random Image" class="w-full h-40 object-cover rounded-t-lg">
                        <div class="p-4">
                            <p class="text-lg font-semibold">"${quote.content}"</p>
                            <p class="mt-2 text-sm text-gray-500">- ${quote.author}</p>
                        </div>
                    `;

                    quoteContainer.appendChild(quoteCard);
                }
            }

            async function fetchQuotes() {
                try {
                    const response = await fetch('https://api.quotable.io/quotes?limit=6');
                    const data = await response.json();

                    const quotes = await Promise.all(data.results.map(async (quote, index) => {
                        const imageResponse = await fetch(`https://source.unsplash.com/random/300x200?sig=${index}`);
                        return {
                            content: quote.content,
                            author: quote.author,
                            image: imageResponse.url
                        };
                    }));

                    displayQuotes(quotes);
                } catch (error) {
                    console.error('Error fetching quotes:', error);
                    displayQuotes(staticQuotes);
                }
            }

            fetchQuotes();
        });
    </script>
</body>
</html>
